{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "webix": true,
    "scheduler": true,
    "VERSION": true,
    "PRODUCTION": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}